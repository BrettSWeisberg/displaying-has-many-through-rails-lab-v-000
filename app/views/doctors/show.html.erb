<label>Doctors Appointment</label>
<ul>
  <%@doctor.appointments.each do |appointment| %>

    <li><%= link_to appointment.patient.name, patient_path(appointment.patient)%></li><br>

    <li><%= appointment.appointment_datetime.to_date.strftime("%B %e, %Y at %I:%M ^(1?[0-9]|2[0-3]):[0-5][0-9]$") %></li>
    <%end%>
</ul>
